Time complexity : O(n^2 logn)
space complexity : O(k)

class Solution {
    public int rangeSum(int[] nums, int n, int left, int right) {
        int mod = 1000000000+7;
        int k = n*(n+1)/2;
        int[] subArr = new int[k];

        int ind = 0;
        for(int i=0;i<n;i++) {
            int sum = 0;
            for(int j=i;j<n;j++) {
                sum += nums[j];
                subArr[ind++] = sum;
            }
        }

        Arrays.sort(subArr);

        int result = 0;

        for(int i=left-1;i<right;i++) {
            result = (result % mod) + subArr[i];
        }
        return result;
    }
}
